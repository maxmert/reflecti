{
  "name": "reflecti",
  "version": "1.0.2",
  "description": "",
  "main": "dist/reflecti.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js src test build",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive ./test/unit",
    "test:bench": "cross-env BABEL_ENV=commonjs npm run babel-node -- test/performance/test.js",
    "test:watch": "npm test -- --watch",
    "build:watch": "webpack --watch",
    "build": "npm run build:umd:min",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/reflecti.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/reflecti.min.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmert/reflecti.git"
  },
  "author": "maxmert (vetrenko.maxim@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxmert/reflecti/issues"
  },
  "homepage": "https://github.com/maxmert/reflecti#readme",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "benchmark": "^2.1.0",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "mocha": "2.4.5",
    "pre-commit": "^1.1.2",
    "redux": "^3.5.1",
    "sinon": "^1.17.3",
    "webpack": "1.12.15"
  },
  "dependencies": {
    "lodash": "^4.11.1",
    "rx": "^4.1.0"
  }
}
